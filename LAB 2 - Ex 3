#include <iostream>
using namespace std;

template <typename T>
T produto(T a, T b) {
    return a * b;
}

int main() {
    int x = 5, y = 3;
    double a = 2.5, b = 4.0;

    cout << "Produto de inteiros: " << produto(x, y) << endl;

    cout << "Produto de doubles: " << produto(a, b) << endl;

    return 0;
}
